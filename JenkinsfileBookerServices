pipeline {
    agent any
    environment {
        BASE_URL_BOOKER = 'https://restful-booker.herokuapp.com/'
        NAME_REPORT = 'report_yape.html'
        FILENAME = "reports/report_yape.html"
    }
    stages {
        stage('Installing') {
            steps {
                echo 'Installing..'
                sh 'pip3 --version'
                sh 'pip3 install -r requirements.txt'
            }
        }
        stage('All Tests...') {
            steps {
                echo 'All Tests..'
                sh 'python3 Runner.py'
            }
}
}
}